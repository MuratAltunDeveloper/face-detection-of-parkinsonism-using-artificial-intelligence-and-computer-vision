ubitak_2209/CNN_BASED_YUZBILGI_EDINIM/just_face_healthy'\n","os.makedirs(healthy_output_folder, exist_ok=True)\n","\n","# Yüz tespiti ve kaydetme fonksiyonu\n","def process_video(video_paths, output_folder):\n","    for video_path in video_paths:\n","        print(f\"Processing video: {video_path}\")\n","\n","        # MediaPipe yüz dedektörü başlat\n","        face_detection = mp_face_detection.FaceDetection(min_detection_confidence=0.5)\n","\n","        cap = cv2.VideoCapture(video_path)\n","        frame_count = 0\n","\n","        while cap.isOpened():\n","            success, frame = cap.read()\n","            if not success:\n","                break\n","\n","            # BGR görüntüyü RGB'ye çevir\n","            rgb_frame = cv2.cvtColor(frame, cv2.COLOR_BGR2RGB)\n","\n","            # Yüzleri algıla\n","            results = face_detection.process(rgb_frame)\n","\n","            if results.detections:\n","              for detection in results.detections:\n","                  # Yüzün koordinatlarını al\n","                  bboxC = detection.location_data.relative_bounding_box\n","                  ih, iw, _ = frame.shape\n","                  x, y, w, h = int(bboxC.xmin * iw), int(bboxC.ymin * ih), int(bboxC.width * iw), int(bboxC.height * ih)\n","\n","                  # Yüz kısmını ayır\n","                  face_frame = frame[y:y+h, x:x+w]\n","\n","                  # Eğer yüz kısmı boşsa, geçerli frame'i atla\n","                  if face_frame is None or face_frame.size == 0:\n","                      continue\n","\n","                  # Yüzü 224x224 boyutunda yeniden boyutlandır\n","                  face_frame_resized = cv2.resize(face_frame, (224, 224))\n","\n","                  # Yüzü kaydet\n","                  output_filename = os.path.join(output_folder, f\"frame_{frame_count:04d}.jpg\")\n","                  cv2.imwrite(output_filename, face_frame_resized)\n","\n","\n","            frame_count += 1\n","\n","        cap.release()\n","\n","# Parkinson videosu üzerinde işlem yap\n","process_video(video_parkinsonspaths, parkinson_output_folder)\n","\n","# Healthy videosu üzerinde işlem yap\n","process_video(video_healthypaths, healthy_output_folder)\n","\n","print(\"Yüz tespiti tamamlandı ve kaydedildi.\")\n"],"metadata":{"colab":{"base_uri":"https://localhost:8080/"},"id":"g3FTee6Fr0nm","executionInfo":{"status":"ok","timestamp":1733637290514,"user_tz":-180,"elapsed":270476,"user":{"displayName":"asistlab","userId":"04370448145332600114"}},"outputId":"a712d1db-b927-4e10-d5eb-030031c6cfc8"},"execution_count":null,"outputs":[{"output_type":"stream","name":"stdout","text":["Processing video: /content/drive/MyDrive/ASISTLAB_ARTIFICAL/tubitak_2209/dataset/parkinsons_head/ali-cebeci-durus-tekayak-1-trim-merged-2_vICXcgYD.mp4\n","Processing video: /content/drive/MyDrive/ASISTLAB_ARTIFICAL/tubitak_2209/dataset/parkinsons_head/bahtiyar-tegun-durus-tekayak-1-trim-merged_4M4vhIE0.mp4\n","Processing video: /content/drive/MyDrive/ASISTLAB_ARTIFICAL/tubitak_2209/dataset/parkinsons_head/hanifi-saglam-durus-tekayak-1-trim-merged-3_MMpNTsmc.mp4\n","Processing video: /content/drive/MyDrive/ASISTLAB_ARTIFICAL/tubitak_2209/dataset/parkinsons_head/cihan-kurt-durus-tekayak-ata-trim-merged_8YDSW17r.mp4\n","Processing video: /content/drive/MyDrive/ASISTLAB_ARTIFICAL/tubitak_2209/dataset/parkinsons_head/ilknur-ustaoglu-durus-tekayak-2-trim-merged-3_gwzE0p7c.mp4\n","Processing video: /content/drive/MyDrive/ASISTLAB_ARTIFICAL/tubitak_2209/dataset/parkinsons_head/hizir-canca-durus-tekayak-3-trim_o9kQzG1m.mp4\n","Processing video: /content/drive/MyDrive/ASISTLAB_ARTIFICAL/tubitak_2209/dataset/parkinsons_head/mehmet-eryigit-durus-tekayak-1-trim-merged_kzLuULsa.mp4\n","Processing video: /content/drive/MyDrive/ASISTLAB_ARTIFICAL/tubitak_2209/dataset/parkinsons_head/hatice-cebeci-durus-tekayak-trim-merged_IDGIqZaw.mp4\n","Processing video: /content/drive/MyDrive/ASISTLAB_ARTIFICAL/tubitak_2209/dataset/parkinsons_head/munip-ozcelik-durus